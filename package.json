  "main": "app/main.jsx",
  "scripts": {
    "install": "git clone https://github.com/iamjoncannon/jaycee_react_boiler.git && cd jaycee_react_boiler && npm run init-project",
    "init-project": "rm -rf .git && git init && npm install && npm run install-lazy && npm start",
    "test-summary": "jest --config=jest.config.js --coverage --watch -- -u",
    "test": "jest $1 --config=jest.config.js --watch -- -u",
    "start": "npm run build & npm run start-server",
    "start-server": "node dev-server.js",
    "build": "webpack -w --progress",
    "site": "open http://$addy",
    "compile": "NODE_ENV=production webpack -p --progress",
    "install-lazy": "npm install webpack@4.28.4 && npm install acorn-dynamic-import@4.0.0 && npm update acorn --depth 20 && npm dedupe"
  },
